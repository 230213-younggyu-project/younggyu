<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.korit.plant.repository.PlantRepository">
    <select id="getAllPlant" parameterType="com.korit.younggyu.plant.web.dto.PlantAllDto">
        select
            id as plantTotalCount,
            plant_region as plantRegion,
            capacity,
            state,
            business
        from
            plant_list
    </select>
</mapper>